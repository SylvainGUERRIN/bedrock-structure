<!doctype html>
<!--[if lt IE 9]-->
<html class="no-js no-svg ie lt-ie9 lt-ie8 lt-ie7" {{ site.language_attributes }}><!--[endif]-->
<!--[if IE 9]-->
<html class="no-js no-svg ie ie9 lt-ie9 lt-ie8" {{ site.language_attributes }}><!--[endif]-->
<!--[if gt IE 9]-->
<html class="no-js no-svg" {{ site.language_attributes }}><!--[endif]-->
    <head>
        <meta charset="{{ site.charset }}">
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="profile" href="http://gmpg.org/xfn/11">
        <link rel="shortcut icon" type="image/png" href="{{ site.theme.link }}/favicon.ico">
        <meta name="theme-color" content="#ffffff">
        {{ function('wp_head') }}
    </head>
    <body class="{{ body_class }}">
    {{ function('wp_nav_menu', {theme_location: 'header', container: false, menu_class: 'menu'}) }}
    {% block body '' %}
    <footer class="footer container">
        Content &copy; {{ "now"|date("Y")}} Magnific portfolio
    </footer>
    {{ function('wp_footer') }}
    </body>
</html>
